<h2>Listado de puntos de interés</h2>
<table>
      <thead>
            <tr>
                  <th>Nombre</th>
                  <th>Ciudad</th>
                  <th>Descripcion</th>
                  <% #Faction %>
                  <th>Tiempo mínimo de check-in</th>
                  <th></th>
            </tr>
      </thead>
      <tbody>
            <% if @pictures.blank? %>
            <tr>
                  <td colspan="6">
                        <em>No se han encontrado puntos de interés</em>
                  </td>
            </tr>
            <% end %>
            <% @pictures.each do |picture| %>
            <tr>
                  <td>
                        <%= picture.name %>
                  </td>
                  <td>
                        <%= picture.city.name %>
                  </td>
                  <!-- Declarado en el modelo
            -->
            <td>
                  <%= picture.description %>
            </td>
            <td>
                  <%= picture.minCheckInTime %>
            </td>

      </tr>
      <% end %>
</tbody>
</table>

<div class="form-row">
      <div class="form-title">
            Edita tus datos
      </div>
    <%= form_for [@user], html: {class: 'form-horizontal' } do |f| %>
      <%= render 'form_edit', f: f %>
      <br/>
      <div class="field">
        <div class="col-md-4 col-md-offset-8">
          <%= f.submit 'Guardar', class:'btn btn-primary' %>
        </div>
      </div>
    <% end %>
  </div>
</div>
